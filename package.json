{
  "name": "protokit",
  "version": "1.0.0",
  "description": "Baseline Prototype Kit using Webpack and FSA Style as a dependency.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.dev.js",
    "build": "webpack --mode production --optimize-minimize --config webpack.prod.js",
    "dev": "npm run build && npm run start",
    "deploy": "gh-pages -d dist",
    "watch": "webpack --watch",
    "stats": "webpack --mode production --optimize-minimize --config webpack.prod.js --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usda-fsa/protokit.git"
  },
  "author": "USDA FSA Enterprise Architecture",
  "contributors": [
    "Casey Hill <bignamehere@gmail.com> (https://github.com/bignamehere)",
    "Francis Rupert <France.Rupert@gmail.com> (https://github.com/francisrupert)"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^8.6.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "fsa-style": "^2.5.1",
    "gh-pages": "^2.2.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "handlebars-webpack-plugin": "^1.8.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@epegzz/sass-vars-loader": "^4.3.0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "pixrem": "^4.0.1",
    "rxjs": "^6.6.0",
    "rxjs-compat": "^6.6.0",
    "sass-resources-loader": "^2.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vue-rx": "^6.2.0",
    "vuex": "^3.5.1"
  }
}
